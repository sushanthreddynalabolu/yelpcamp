 <% layout('./layouts/boilerplate') %>
 <div id="cluster-map"></div>
    <h1>Campgrounds are working</h1>
        <% for(let camp of campground){ %>
            <div class="card mb-3">
                <div class="row">
                    <div class="col-md-4">
                        <% if(camp.images.length) {%>
                        <img  crossorigin="anonymous"  src="<%= camp.images[0].url %>" class="img-fluid" alt="">
                        <% }else{ %>
                            <img src="http://thewowstyle.com/wp-content/uploads/2015/01/nature-images..jpg" class="img-fluid" alt="">
                            <% } %>
                    </div>
                    <div class="col-md-8">
                        <div class="card-body">
                            <div class="card-title"><%= camp.title %></div>
                            <p class="card-text"><%= camp.description %></p>
                            <p class="card-text">
                                <small class="muted-text"><%= camp.location %></small>
                            </p>
                            <a  href="/campground/<%=camp._id %>" class="btn btn-primary">view <%= camp.title %></a>
                        </div>
                    </div>
                </div>
            </div>
            <% } %> 
            <script>
               const mapToken='<%-process.env.MAPBOX_TOKEN %>';
                const campground={features:<%-JSON.stringify(campground) %>};
              
            </script>
             
             <script src="/javascripts/clusterMap.js"></script>
    
 